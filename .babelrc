{
  "presets": [
    "next/babel",
  ],
  "plugins": [
    ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ],
    ["module-resolver", { 
        "root": ["."],
        "alias": {
          "components": "./app/components/",
          "interfaces": "./app/interfaces/",
          "static": "./app/static/",
          "utils": "./app/utils/"
        } 
      }
    ]
  ]
}
